ontainer\">\n    <h1 class=\"mt-4 text-dark\">\n      Send Blood Request to {{ this.bloodOrgName }}\n    </h1>\n    <div v-if=\"showAlert\" class=\"alert alert-dismissible alert-success\">\n      <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\"></button>\n      <strong>{{ alertMessage }}</strong>\n    </div>\n    <div class=\"row mb-4\">\n      <div class=\"col\">\n        <img src=\"../../assets/istockphoto-538340960-170667a.jpg\" alt=\"\" />\n      </div>\n      <div class=\"col mt-5\" v-if=\"this.bloodOrganizationEmail\">\n        <form @submit=\"sendBloodReq($event)\">\n          <div class=\"form-group row mt-5\">\n            <label for=\"bloodOrgEmail\" class=\"col-sm-5 col-form-label text-dark\"\n              >Enter Blood Organization Email:</label\n            >\n            <div class=\"col-sm-6\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"bloodOrgEmail\"\n                value=\"this.bloodOrganizationEmail\"\n                v-model=\"this.bloodOrganizationEmail\"\n                disabled\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group row mt-4\">\n            <label for=\"patientName\" class=\"col-sm-5 col-form-label text-dark\"\n              >Patient Name:</label\n            >\n            <div class=\"col-sm-6\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"patientName\"\n                v-model=\"BloodRequestClass.patientName\"\n                required\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group row mt-4\">\n            <label for=\"patientNumber\" class=\"col-sm-5 col-form-label text-dark\"\n              >Patient Number :</label\n            >\n            <div class=\"col-sm-6\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"patientNumber\"\n                v-model=\"BloodRequestClass.patientNumber\"\n                required\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group row mt-4\">\n            <label\n              for=\"patientAddress\"\n              class=\"col-sm-5 col-form-label text-dark\"\n              >Patient Address :</label\n            >\n            <div class=\"col-sm-6\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"patientAddress\"\n                v-model=\"BloodRequestClass.patientAddress\"\n                required\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group row mt-4\">\n            <label for=\"bloodGroup\" class=\"col-sm-5 col-form-label text-dark\"\n              >Requested Blood Group :</label\n            >\n            <div class=\"col-sm-6\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"bloodGroup\"\n                value=\"this.requestBloodType\"\n                v-model=\"this.requestBloodType\"\n                disabled\n              />\n            </div>\n          </div>\n\n          <button type=\"submit\" class=\"btn btn-success mt-5\">\n            Send Blood Request\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n  <footer class=\"footer bg-success py-4\">\n    <div class=\"container text-center\">\n      <span class=\"text-muted\">This is the footer content.</span>\n    </div>\n  </footer>\n</template>\n\n<script>\nimport backendRestApi from \"@/service/backendRestApi\";\nexport default {\n  name: \"bloodRequest\",\n  data() {\n    return {\n      BloodRequestClass: {\n        bloodOrganizationEmail: \"\",\n        patientName: \"\",\n        patientNumber: \"\",\n        patientAddress: \"\",\n        requestBloodType: \"\",\n      },\n      bloodOrgName: \"\",\n      showAlert: \"\",\n      alertMessage: \"\",\n    };\n  },\n  created() {\n    this.bloodOrgName = this.$route.params.orgName;\n    this.bloodOrganizationEmail = this.$route.params.orgEmail;\n    this.requestBloodType = this.$route.params.bloodType;\n  },\n  methods: {\n    async sendBloodReq(event) {\n      event.preventDefault();\n      backendRestApi\n        .sendBloorRequest(this.BloodRequestClass)\n        .then((response) => {\n          if (response.data) {\n            this.showAlert = true;\n            this.alertMessage = \"Blood Request Sended\";\n            setTimeout(() => {\n              this.showAlert = false;\n              this.alertMessage = \"\";\n              this.BloodRequestClass = {\n                bloodOrganizationEmail: \"\",\n                patientName: \"\",\n                patientNumber: \"\",\n                patientAddress: \"\",\n                requestBloodType: \"\",\n              };\n            }, 3000);\n          }\n        })\n        .catch((error) => {\n          this.showAlert = true;\n          this.alertMessage = \"Error : \" + error.message;\n        });\n    },\n  },\n};\n</script>\n\n<style></style>\n\n<!-- private String bloodOrganizationEmail;\nprivate String patientName;\nprivate String patientNumber;\nprivate String patientAddress;\nprivate String requestBloodType -->\n"],"mappings":"AA6GA,OAAOA,cAAa,MAAO,0BAA0B;AACrD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE;QACjBC,sBAAsB,EAAE,EAAE;QAC1BC,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,EAAE;QACjBC,cAAc,EAAE,EAAE;QAClBC,gBAAgB,EAAE;MACpB,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACH,YAAW,GAAI,IAAI,CAACI,MAAM,CAACC,MAAM,CAACC,OAAO;IAC9C,IAAI,CAACX,sBAAqB,GAAI,IAAI,CAACS,MAAM,CAACC,MAAM,CAACE,QAAQ;IACzD,IAAI,CAACR,gBAAe,GAAI,IAAI,CAACK,MAAM,CAACC,MAAM,CAACG,SAAS;EACtD,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAACC,KAAK,EAAE;MACxBA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBrB,cAAa,CACVsB,gBAAgB,CAAC