{{ card.division }}</p>\n              <p class=\"card-text\">{{ card.distric }}</p>\n              <p class=\"card-text\">{{ card.Upazila }}</p>\n\n              <button\n                @click=\"goTobloodRequest(cards.title)\"\n                class=\"btn btn-success\"\n                type=\"button\"\n              >\n                Send Request\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <!-- Remove the container if you want to extend the Footer to full width. -->\n</template>\n\n<script>\nimport backendRestApi from '@/service/backendRestApi';\nexport default {\n  name: \"searchBlood\",\n  data() {\n    return {\n      searchValue:\"\",\n      searchOption:\"\",\n      bloodOrg:[],\n    };\n  },\n  methods: {\n    searchOrg(){\n      switch(this.searchOption){\n        case \"division\":\n          this.searchByDivision(this.searchValue);\n          break;\n        case \"distric\":\n          this.searchByDistric(this.searchValue);\n          break;\n        case \"upazila\":\n          this.searchByUpazila(this.searchValue);\n          break;\n        case \"bloodType\":\n          this.searchByBloodType(this.searchValue);\n          break;\n      }\n    },\n    async searchByDivision(searchValue){\n        try{\n          const response = await backendRestApi.searchByDivision(searchValue);\n          this.bloodOrg = response.data.map((organizations)=>({\n            BloodOrganizationClass:{\n              bloodOrganizationId: organizations.bloodOrganizationId || \"\",\n              bloodOrganizationName: organizations.bloodOrganizationName || \"\",\n              bloodOrganizationEmail: organizations.bloodOrganizationEmail || \"\",\n              bloodOrganizationDivision: organizations.bloodOrganizationDivision || \"\",\n              bloodOrganizationDistric: organizations.bloodOrganizationDistric || \"\",\n              bloodOrganizationUpozila: organizations.bloodOrganizationUpozila || \"\",\n              bloods: organizations.bloods || \"\"\n            },\n          }))\n        }catch(error){\n          console.log(error);\n        }\n    },\n    goTobloodRequest(cardTitle) {\n      this.$router.push({\n        name: \"bloodRequest\",\n        params: { Ctitle: cardTitle },\n      });\n    },\n  },\n};\n</script>\n\n<style></style>\n"],"mappings":";AA4DA,OAAOA,cAAa,MAAO,0BAA0B;AACrD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAC,EAAE;MACdC,YAAY,EAAC,EAAE;MACfC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAE;MACT,QAAO,IAAI,CAACH,YAAY;QACtB,KAAK,UAAU;UACb,IAAI,CAACI,gBAAgB,CAAC,IAAI,CAACL,WAAW,CAAC;UACvC;QACF,KAAK,SAAS;UACZ,IAAI,CAACM,eAAe,CAAC,IAAI,CAACN,WAAW,CAAC;UACtC;QACF,KAAK,SAAS;UACZ,IAAI,CAACO,eAAe,CAAC,IAAI,CAACP,WAAW,CAAC;UACtC;QACF,KAAK,WAAW;UACd,IAAI,CAACQ,iBAAiB,CAAC,IAAI,CAACR,WAAW,CAAC;UACxC;MACJ;IACF,CAAC;IACD,MAAMK,gBAAgBA,CAACL,WAAW,EAAC;MAC/B,IAAG;QACD,MAAMS,QAAO,GAAI,MAAMZ,cAAc,CAACQ,gBAAgB,CAACL,WAAW,CAAC;QACnE,IAAI,CAACE,QAAO,GAAIO,QAAQ,CAACV,IAAI,CAACW,GAAG,CAAEC,aAAa,KAAI;UAClDC,sBAAsB,EAAC;YACrBC,mBAAmB,EAAEF,aAAa,CAACE,mBAAkB,IAAK,EAAE;YAC5DC,qBAAqB,EAAEH,aAAa,CAACG,qBAAoB,IAAK,EAAE;YAChEC,sBAAsB,EAAEJ,aAAa,CAACI,sBAAqB,IAAK,EAAE;YAClEC,yBAAyB,EAAEL,aAAa,CAACK,yBAAwB,IAAK,EAAE;YACxEC,wBAAwB,EAAEN,aAAa,CAACM,wBAAuB,IAAK,EAAE;YACtEC,wBAAwB,EAAEP,aAAa,CAACO,wBAAuB,IAAK,EAAE;YACtEC,MAAM,EAAER,aAAa,CAACQ,MAAK,IAAK;UAClC;QACF,CAAC,CAAC;MACJ,CAAC,QAAMC,KAAK,EAAC;QACXC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACJ,CAAC;IACDG,gBAAgBA,CAACC,SAAS,EAAE;MAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAChB5B,IAAI,EAAE,cAAc;QACpB6B,MAAM,EAAE;UAAEC,MAAM,EAAEJ;QAAU;MAC9B,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}